{
  "name": "htmldown",
  "version": "0.0.2",
  "main": "index.js",
  "jsnext:main": "src/htmldown.js",
  "description": "My spectacular module",
  "license": "MIT",
  "repository": "egoist/htmldown",
  "author": {
    "name": "EGOIST",
    "email": "0x142857@gmail.com",
    "url": "github.com/egoist"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "bin": {
    "htmldown": "./cli.js"
  },
  "scripts": {
    "test": "xo && ava tests/test*.js",
    "build:cjs": "rollup -c",
    "build:browser": "npm run build:cjs && webpack"
  },
  "files": [
    "index.js",
    "src",
    "htmldown.browser.js"
  ],
  "keywords": [
    "markdown",
    "html"
  ],
  "dependencies": {
    "cheerio": "^0.19.0"
  },
  "devDependencies": {
    "ava": "^0.10.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "cz-conventional-changelog": "^1.1.5",
    "json-loader": "^0.5.4",
    "rollup": "^0.25.0",
    "rollup-plugin-babel": "^2.3.9",
    "rollup-plugin-json": "^2.0.0",
    "webpack": "^1.12.11",
    "xo": "^0.12.1"
  },
  "xo": {
    "semicolon": false,
    "esnext": true,
    "ignores": [
      "index.js",
      "htmldown.browser.js"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "tooling": {
    "index": {
      "title": "htmldown",
      "template": "example/index.html"
    }
  }
}
